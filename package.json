{
  "name": "sofe-inspector",
  "version": "2.1.7",
  "description": "An extension for the Chrome Developer Tools for managing sofe services",
  "main": "dist/sofe-inspector.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --config webpack.config.js -p",
    "watch": "webpack --config webpack.config.js -w",
    "start": "webpack-dev-server --config ./webpack.config.js --https --cert ~/.canopy-ssl/public.pem --key ~/.canopy-ssl/key.pem --port",
    "deploy": "surge dist",
    "clean": "rm dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CanopyTax/sofe-inspector.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CanopyTax/sofe-inspector/issues"
  },
  "homepage": "https://github.com/CanopyTax/sofe-inspector#readme",
  "dependencies": {
    "clean-webpack-plugin": "0.1.16",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-draggable": "3.0.3",
    "vis": "4.20.1",
    "whatwg-fetch": "2.0.3"
  },
  "sofe": {
    "url": "https://unpkg.com/sofe-inspector@2.1.8/dist/sofe-inspector.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.6",
    "file-loader": "^0.11.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1"
  },
  "files": [
    "dist",
    "src"
  ]
}
